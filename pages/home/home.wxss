}
.part{
  border-radius:10px 10px 0 0;
}
.bottompart{
  border-radius:10px 10px;
}
.k-box-row{
  display: flex;
  flex-flow: row nowrap;
  border-bottom: 1px solid #eee;
}
.k-box-cell{
  flex: 1 1 auto;
  text-align: center;
  min-width: 33.3%;
  min-height: 90px;
  position: relative;
  user-select: none;
  border-right:  0.01em solid #eee;
  align-items: center;
}

        
.k-box-left{
  border-left: 0em solid #c7c7c7;
  flex: 1 1 auto;
  text-align: center;
  min-width: 33.3%;
  min-height: 90px;
  position: relative;
  user-select: none;
  /* border-right:  0.01em solid #eee; */
  align-items: center;
}
.k-box-inner{
  flex: 1 1 auto;
  text-align: center;
  min-width: 33.3%;
  min-height: 90px;
  position: relative;
  user-select: none;
  /* border-right:  0.01em solid #eee; */
  align-items: center;
  }
.k-box-right{
  border-right: 0em solid #c7c7c7;
  flex: 1 1 auto;
  text-align: center;
  min-width: 33.3%;
  min-height: 90px;
  position: relative;
  user-select: none;
  /* border-right:  0.01em solid #eee; */
  align-items: center;
  }
  .round-click{
    height: 85rpx;
    width: 85rpx;
    
    border-radius: 100%;
    position: fixed;
    bottom: 150rpx;
    right: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 9;
  }
    
  .round-click navigator{
    font-size: 32rpx;
    max-width: 80rpx;
    color: #fff;
    text-align: center;
    }
    .upimg{
      height: 85rpx;
      width: 85rpx;
      opacity: 0.7;
    }
    /* 下面的商品 */
    .has_shop_list{
      width: 100%;
      min-height: 100%;
      margin-top: 20rpx;
      display: flex;
      flex-direction: column;
      /* padding-bottom: 200rpx; */
    }
    .img_item {
      width: 48.5%;
      margin: 2px;
      flex-direction: row;
      display: inline-block;
      background-color: #ffffff;
      font-size: 24rpx;
    
    }
    .item_info{
      border-top:0px dashed red;
    }
    .product-name{
      color: #000;
      /* height: 28px; */
      text-align:left;
       margin: 0px 5px; 
       margin-bottom: 5px; 
       text-overflow: -o-ellipsis-lastline;
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 1;
        -webkit-box-orient: vertical;
    }
    .product-price-wrap .product-price-new{
      color: #e80080;
      margin-left:5px;
      font-weight:900;
    }
    .product-price-wrap .product-price-old{
      color: #888;
      text-decoration: line-through;
      padding-left: 2px;
    }
    .product-price-wrap .discount{
      margin-left: 30px;
      background-color: #000;
      color: #fff;
    }
    /* 公告 */
    .notice {
      display: flex;
      align-content: center;
      justify-content: space-between;
      padding: 10rpx 25rpx;
      background: #f1f1f1;
    }
    
    .left {
      display: flex;
      align-items: center;
    }
    
    .voice {
      margin-right: 5rpx;
      margin-top: 2rpx;
      color: #fa6016;
    }
    
    .left-box {
      position: relative;
      display: flex;
      align-items: center;
    }
    
    .left-text {
      position: absolute;
      left: 0;
      width: 40rpx;
      height: 100%;
      background: linear-gradient(to left,rgba(241,241,241,0),rgba(241,241,241,1));
      z-index: 99;
    }
    
    .right-text {
      position: absolute;
      right: -1rpx;
      width: 40rpx;
      height: 100%;
      background: linear-gradient(to left,rgba(241,241,241,1),rgba(241,241,241,0));
      z-index: 99;
    }
    
    .content-box {
      overflow: hidden;
      width: 750rpx;
    }
    
    .content-text {
      color: #5e5e5e;
      white-space: nowrap;
      font-size: 28rpx;
    }
    
    .right {
      display: flex;
      align-items: center;
    }
    
    .app {
      height: 48rpx;
    }
    
    .more {
      margin-left: 5rpx;
      color: #aaa;
      font-size: 32rpx;
    }

